@using AccountsAppWeb.Infrastructure;
@{
    ViewBag.Title = "BudgetDetailsReport";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Budget Details Report</h1>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default panel-primary2">
            <div class="panel-heading">Budget Details  Report</div>
            <div class="panel-body">
                <div class="table-container">
                    <div class="row table-top reconciliation-pag-cls">
                        
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label>
                                    Org.Name:
                                </label>
                                <select id="ddlInstitute" class="form-control"></select>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <button type="button" class="btn btn-primary btn-column" id="btnView">View</button>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default panel-primary2">
            <div class="panel-heading">
                Budget Details Income Report 
            </div>
            <div class="panel-body">
                <div class="table-container">
                    <table id="tblBudgetDetailsReportInc" class="table table-striped table-bordered table-hover main-table">
                        <thead>
                            <tr>
                                <th>Ledger Name</th>
                                <th>Org.Name</th>
                                <th>Total Budget(Current Financial Year)</th>
                                <th>8 Months Transaction</th>
                                <th>4 Months Total Budget</th>
                                <th>@UserManager.User.FinTitle1 Total Budget</th>
                                <th>@UserManager.User.FinTitle2 Total Budget</th>
                                <th>@UserManager.User.FinTitle3 Total Budget</th>
                                <th>@UserManager.User.FinTitle4 Total Budget</th>
                                <th>Net.Total</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default panel-primary2">
            <div class="panel-heading">
                Budget Details Expenditure Report
            </div>
            <div class="panel-body">
                <div class="table-container">
                    <table id="tblBudgetDetailsReportExp" class="table table-striped table-bordered table-hover main-table">
                        <thead>
                            <tr>
                                <th>Ledger Name</th>
                                <th>Org.Name</th>
                                <th>Total Budget(Current Financial Year)</th>
                                <th>8 Months Transaction</th>
                                <th>4 Months Total Budget</th>
                                <th>@UserManager.User.FinTitle1 Total Budget</th>
                                <th>@UserManager.User.FinTitle2 Total Budget</th>
                                <th>@UserManager.User.FinTitle3 Total Budget</th>
                                <th>@UserManager.User.FinTitle4 Total Budget</th>
                                <th>Net.Total</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/CustomScripts/BudgetDetailsReport.js"></script>